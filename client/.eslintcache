ketContext.Provider value={{socket,gameData,setGameData}}>\n      <Router>\n        <div className=\"flex-column justify-flex-start min-100-vh\">\n          <Header />\n          <div className=\"container\">\n            <Routes>\n              <Route \n                path=\"/\" \n                element={<Home />}\n              />\n              <Route \n                path=\"/login\" \n                element={<Login />}\n              />\n              <Route \n                path=\"/signup\" \n                element={<Signup />}\n              />\n              <Route \n                path=\"/me\" \n                element={<Profile />}\n              />\n              <Route \n                path=\"/profiles/:profileId\"\n                element={<Profile />}\n              />\n              <Route \n                path=\"/lobby/:lobbyId\"\n                element={<Lobby />}\n              />\n              <Route \n                path=\"/game/:lobbyId\"\n                element={<Game />}\n              />\n            </Routes>\n          </div>\n          <Footer />\n        </div>\n      </Router>\n      </socketContext.Provider>\n    </ApolloProvider>\n  );\n}\n\nexport default App;\n","C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Login.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Signup.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\utils\\auth.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Profile.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Game.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Home.js",["111","112"],"import React, { useContext, useEffect, useState } from 'react';\nimport ParticleBackground from 'react-particle-backgrounds'\nimport { socketContext } from '../utils/socketContext';\nimport { Link, Navigate } from 'react-router-dom';\nimport auth from '../utils/auth';\n\nconst Home = () => {\n  const { socket, gameData, setGameData } = useContext(socketContext);\n  const [ID, setID] = useState('');\n  const [validCode, setvalidCode] = useState(false);\n  const [error, setError] = useState('');\n  const settings = {\n    canvas: {\n      canvasFillSpace: true,\n      width: 200,\n      height: 500,\n      useBouncyWalls: false\n    },\n    particle: {\n      particleCount: 100,\n      color: \"#d68c38\",\n      minSize: 2,\n      maxSize: 4\n    },\n    velocity: {\n      directionAngle: 0,\n      directionAngleVariance: 30,\n      minSpeed: 0.2,\n      maxSpeed: 7\n    },\n    opacity: {\n      minOpacity: 0,\n      maxOpacity: 0.5,\n      opacityTransitionTime: 5000\n    }\n  }\n\n  function handleChange(e) {\n    setID(e.target.value);\n    if (/^[A-Za-z]{4}$/.test(e.target.value)) {\n      setvalidCode(true);\n    } else {\n      setvalidCode(false);\n    }\n  }\n\n  function attemptJoin(lobbyCode) {\n    if (lobbyCode) {\n      setGameData({ ...gameData, lobbyCode });\n      localStorage.setItem('lobbycode', lobbyCode);\n      window.location.assign(`lobby/${lobbyCode}`)\n    } else {\n      setError(\"Could not find lobby\");\n    }\n  }\n  function joinLobby(lobbyCode) {\n    socket.emit('joinLobby', auth.getUsername(), lobbyCode, attemptJoin);\n  }\n\n  function attemptHost(lobbyCode) {\n    if (lobbyCode) {\n      setGameData({ ...gameData, lobbyCode });\n      localStorage.setItem('lobbycode', lobbyCode);\n      window.location.assign(`lobby/${lobbyCode}`);\n    } else {\n      setError('Already in a lobby!');\n    }\n  }\n  function hostLobby() {\n    socket.emit('newLobby', auth.getUsername(), false, attemptHost);\n  }\n\n  function attemptLeave(status){\n    if(status){\n      localStorage.removeItem('lobbycode');\n      setError('Lobby has be left.');\n      setGameData({gameState: 'Testing',lobbyCode: localStorage.getItem('lobbycode')});\n    }\n  }\n  function leaveGame() {\n    socket.emit('leaveLobby', auth.getUsername(), gameData.lobbyCode, attemptLeave);\n    if(gameData.host.username === auth.getUsername()){\n      socket.emit('endGame', gameData.lobbyCode);\n    }\n  }\n  return (\n    <main>\n      <div className=\"container\">\n        <div className=\"row\">\n          <div className=\"d-grid gap-5 col-12 mx-auto\">\n            {auth.loggedIn() ? <>\n              <h3 className='text-light'>{error}</h3>\n              {gameData.lobbyCode ? <>\n                <button className=\"align-self-end btn btn-danger btn-lg py-5\" onClick={() => joinLobby(gameData.lobbyCode)}><h1>RE-JOIN GAME</h1></button>\n                <button className=\"align-self-end btn btn-danger btn-lg py-5\" onClick={leaveGame}><h1>LEAVE GAME</h1></button>\n              </> : <>\n              <input onChange={handleChange} value={ID}></input>\n              <button className=\"align-self-end btn btn-danger btn-lg py-5\" onClick={() => joinLobby(ID.toUpperCase())} disabled={!validCode}><h1>JOIN GAME</h1></button>\n              <button className=\"align-self-end btn btn-danger btn-lg py-5\" onClick={hostLobby}><h1>HOST GAME</h1></button>         \n              </>}\n              <Link className=\"align-self-end btn btn-danger btn-lg py-5\" to={`/game/${gameData.lobbyCode}`}><h1>TEST GAME</h1></Link>\n            </> : <>\n              <Link className=\"align-self-end btn btn-danger btn-lg py-5\" to=\"/login\"><h1>Login or Signup to play!</h1></Link>\n\n            </>}\n\n          </div>\n        </div>\n      </div>\n      <ParticleBackground settings={settings} />\n      <div className=\"flex-row justify-center\">\n        <div className=\"col-12 col-md-10 my-3\">\n        </div>\n      </div>\n    </main>\n\n  );\n};\n\nexport default Home;\n","C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Lobby.js",["113","114","115"],"import React, { useContext, useEffect } from \"react\";\nimport auth from \"../utils/auth\";\nimport { Navigate, useParams } from \"react-router-dom\";\nimport { socketContext } from \"../utils/socketContext\";\nimport { Link } from \"react-router-dom\";\n\nconst Lobby = () => {\n  const { socket, gameData, setGameData } = useContext(socketContext);\n\n\n\n  useEffect(() => {\n    socket.on('lobbyUpdate', clientData => {\n      setGameData(clientData);\n      if (!clientData.lobbyCode) {\n        window.location.reload();\n      }\n    });\n    socket.on('requestPrompt', clientData => {\n      window.location.replace(`/game/${clientData.lobbyCode}`);\n    });\n    socket.on('requestPromptFC', clientData => {\n      window.location.replace(`/game/${clientData.lobbyCode}`);\n    });\n    return () => {\n      socket.off('lobbyUpdate', clientData => {\n        setGameData(clientData);\n        if (!clientData.lobbyCode) {\n          window.location.reload();\n        }\n      });\n      socket.off('requestPrompt', clientData => {\n        window.location.replace(`/game/${clientData.lobbyCode}`);\n      });\n      socket.off('requestPromptFC', clientData => {\n        window.location.replace(`/game/${clientData.lobbyCode}`);\n      });\n    }\n  }, [socket, setGameData]);\n\n  function StartGame(){\n    socket.emit('startRound', gameData.lobbyCode);\n  }\n\n  if (!gameData.lobbyCode) {\n    return <h1>This lobby no longer exists... sorry!</h1>;\n  }\n  if (!gameData.players) {\n    return <h1>Loading game data...</h1>;\n  }\n  return (\n    <>\n      <h1 className=\"text-light text-center\">\n        Are you ready for your seat to get warm?\n      </h1>\n\n      {auth.loggedIn() ? (\n        <>\n          <div className=\"centered\">\n            <h1 className=\"text-light text-center bg-primary\">Lobby Code: {gameData.lobbyCode} </h1>\n            {/* Placeholder for dynamic to be added */}\n\n            <div className=\"text-center bg-light\">\n              {gameData.players.map(item => {\n                return <h1>{item.username}</h1>\n              })}\n            </div>\n          </div>\n          {/* Plug in socket to dynamically add user when lobby is joined */}\n          {gameData.host.username === auth.getUsername() ?\n            <button onClick={StartGame} className=\"btn-danger btn-lg py-5 align-item-center justify-content-center\">\n              <h1>START</h1>\n            </button> : <></>}\n\n        </>\n      ) : (\n        <></>\n      )}\n    </>\n  );\n};\n\nexport default Lobby;\n","C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\utils\\socketContext.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\utils\\mutations.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\components\\Footer\\index.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\components\\Header\\index.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\utils\\queries.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\components\\Header\\Logo.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\components\\DeckList\\index.js",["116"],"import React from 'react';\nimport { useMutation } from '@apollo/client';\n\nimport { REMOVE_FROM_DECK, REMOVE_PROMPT } from '../../utils/mutations';\nimport { QUERY_ME } from '../../utils/queries';\n\nconst DeckList = ({ deck, isLoggedInUser = false }) => {\n  const [removeFromDeck, { error }] = useMutation(REMOVE_FROM_DECK, {\n    update(cache, { data: { removeFromDeck } }) {\n      try {\n        cache.writeQuery({\n          query: QUERY_ME,\n          data: { me: removeFromDeck },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n    },\n  });\n\n  const [removePrompt] = useMutation(REMOVE_PROMPT, {\n    update(cache, {data: {removePrompt}}) {\n      try {\n        cache.writeQuer