ate(cache, { data: { removeFromDeck } }) {\r\n      try {\r\n        cache.writeQuery({\r\n          query: QUERY_ME,\r\n          data: { me: removeFromDeck },\r\n        });\r\n      } catch (e) {\r\n        console.error(e);\r\n      }\r\n    },\r\n  });\r\n\r\n  const [removePrompt, { err }] = useMutation(REMOVE_PROMPT, {\r\n    update(cache, {data: {removePrompt}}) {\r\n      try {\r\n        cache.writeQuery({\r\n          query: QUERY_ME,\r\n          data: { me: removePrompt},\r\n        });\r\n      } catch (e) {\r\n        console.error(e);\r\n      }\r\n    },\r\n  });\r\n\r\n  const handleRemovePrompt = async (prompt) => {\r\n    try {\r\n      await removeFromDeck({\r\n        variables: { prompt },\r\n      });\r\n\r\n      const { data } = await removePrompt({\r\n        variables: {prompt},\r\n      });\r\n    } catch (err) {\r\n      console.error(err);\r\n    }\r\n  };\r\n\r\n  if (!deck.length) {\r\n    return <h3>Nothing in Your Deck Yet</h3>;\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <div className=\"flex-row justify-space-between my-4\">\r\n        {deck &&\r\n          deck.map((prompt) => (\r\n            <div key={prompt} className=\"col-12 col-xl-6\">\r\n              <div className=\"card mb-3\">\r\n                <h4 className=\"card-header bg-dark text-light p-2 m-0 display-flex align-center\">\r\n                  <span>{prompt}</span>\r\n                  {isLoggedInUser && (\r\n                    <button\r\n                      className=\"btn btn-sm btn-danger ml-auto\"\r\n                      onClick={() => handleRemovePrompt(prompt)}\r\n                    >\r\n                      X\r\n                    </button>\r\n                  )}\r\n                </h4>\r\n              </div>\r\n            </div>\r\n          ))}\r\n      </div>\r\n      {error && (\r\n        <div className=\"my-3 p-3 bg-danger text-white\">{error.message}</div>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default DeckList;\r\n","C: