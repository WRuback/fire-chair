Code, setvalidCode] = useState(false);\n  const [error, setError] = useState('');\n  const settings = {\n    canvas: {\n      canvasFillSpace: true,\n      width: 200,\n      height: 500,\n      useBouncyWalls: false\n    },\n    particle: {\n      particleCount: 100,\n      color: \"#d68c38\",\n      minSize: 2,\n      maxSize: 4\n    },\n    velocity: {\n      directionAngle: 0,\n      directionAngleVariance: 30,\n      minSpeed: 0.2,\n      maxSpeed: 7\n    },\n    opacity: {\n      minOpacity: 0,\n      maxOpacity: 0.5,\n      opacityTransitionTime: 5000\n    }\n  }\n\n  function handleChange(e){\n    setID(e.target.value);\n    if(/^[A-Za-z]{4}$/.test(e.target.value)){\n      setvalidCode(true);\n    }else{\n      setvalidCode(false);\n    }\n  }\n\n  function attemptJoin(lobbyCode){\n    lobbyCode ? window.location.assign(`lobby/${lobbyCode}`) : setError(\"Could not find lobby\");\n  }\n  function joinLobby(){\n    console.log(\"running function\");\n    socket.emit('joinLobby', auth.getUsername(), ID.toUpperCase(), attemptJoin);\n  }\n\n  // function attemptJoin(lobbyCode){\n  //   lobbyCode ? window.location.assign(`lobby/${lobbyCode}`) : setError(\"Could not find lobby\");\n  // }\n  // function joinLobby(){\n  //   console.log(\"running function\");\n  //   socket.emit('joinLobby', auth.getUsername(), ID.toUpperCase(), attemptJoin);\n  // }\n\n  return (\n    <main>\n      <div className=\"container\">\n        <div className=\"row\">\n          <div className=\"d-grid gap-5 col-12 mx-auto\">\n            {auth.loggedIn() ? <>\n            <input onChange={handleChange} value={ID}></input>\n            <button className=\"align-self-end btn btn-danger btn-lg py-5\" onClick={joinLobby} disabled={!validCode}><h1>JOIN GAME</h1></button>\n            <button className=\"align-self-end btn btn-danger btn-lg py-5\"><h1>HOST GAME</h1></button>\n            <Link className=\"align-self-end btn btn-danger btn-lg py-5\" to={`/game/${gameData.lobbyCode}`}><h1>TEST GAME</h1></Link>\n            </> : <>\n            <Link className=\"align-self-end btn btn-danger btn-lg py-5\"to=\"/login\"><h1>Login or Signup to play!</h1></Link>\n            \n            </>}\n          \n          </div>\n        </div>\n      </div>\n      <ParticleBackground settings={settings} />\n      <div className=\"flex-row justify-center\">\n        <div className=\"col-12 col-md-10 my-3\">\n        </div>\n      </div>\n    </main>\n\n  );\n};\n\nexport default Home;\n","C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Game.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Login.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Signup.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Profile.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\utils\\auth.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\pages\\Lobby.js",["112","113"],"import React from 'react';\nimport auth from '../utils/auth';\nimport { Navigate, useParams } from 'react-router-dom';\nimport { socketContext } from '../utils/socketContext';\n\nconst Lobby = () => {\n    return (\n        <>\n            <p>Is this working?</p>\n            {auth.loggedIn() ? \n            <>\n                <p>Welcome! Lets get a lobby going!</p>\n            </>:\n            <>\n                <Navigate to=\"/login\" />\n            </>}\n        </>\n    );\n};\n\nexport default Lobby;","C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\components\\Header\\index.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\components\\Footer\\index.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fire-chair\\client\\src\\utils\\mutations.js",[],"C:\\Users\\ezequ\\Desktop\\UCF\\Projects\\Project 3\\fir